<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cmpe282.cloydsurvey"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuestionText"
            android:label="@string/title_activity_question_text" >
        </activity>
        <activity
            android:name=".QuestionOptions"
            android:label="@string/title_activity_question_options" >
        </activity>
        <activity
            android:name=".QuestionYesNo"
            android:label="@string/title_activity_question_yes_no" >
        </activity>
        <activity
            android:name=".Questions"
            android:label="@string/title_activity_questions" >
        </activity>
        <activity
            android:name=".QuestionChoice"
            android:label="@string/title_activity_question_choice" >
        </activity>
    </application>

</manifest>
